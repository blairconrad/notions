{
  "name": "Tidy Agfa Build Results",
  "version": "0.7.2011.312",
  "description": "Tidies and adds links to the Agfa CruiseControl build results",
  "background_page": "background.html",
  "page_action": 
  {
    "_default_icon": "icon16.png",
    "default_title": "Tidy Agfa Build Results"
  },
  "content_scripts": [
  {
    "js": ["jquery-1.4.2.min.js", "tidy.js"],
    "matches": ["http://*/cruisecontrol/buildresults/*", "http://*/buildresults/*"]
  }
   ],
   "permissions": [ "tabs" ],
    "icons": 
    {
        "16": "icon_16.png",
        "48": "icon_48.png",
        "128": "icon_128.png"
    }
}